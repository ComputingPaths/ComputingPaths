doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    link(
      rel='stylesheet'
      href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css'
      integrity='sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ'
      crossorigin='anonymous')
    link(
      rel='stylesheet'
      href='/css/template.css')
    link(
      rel='stylesheet'
      href='https://fonts.googleapis.com/css?family=Roboto:400,500,700'
    )
    block style

    script(
      src='https://code.jquery.com/jquery-3.2.1.min.js')
    script(
      src='https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js'
      integrity='sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb'
      crossorigin='anonymous')
    script(
      src='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js' 
      integrity='sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn'
      crossorigin='anonymous')
    script(src='https://use.fontawesome.com/8f54e6c37c.js')
    script(src='/js/template.js')
    block script
    
    title Computing Paths

  body
    nav(class='navbar fixed-top navbar-toggleable-md')
      button(
        class='navbar-toggler navbar-toggler-right'
        type='button'
        data-toggle='collapse'
        data-target='#navbarSupportedContent'
        aria-controls='navbarSupportedContent'
        aria-expanded='false' 
        aria-label='Toggle navigation')
          i(class='fa fa-bars')
      div(class='navbar-brand')
        a(href='/')
          img(src='/img/logo.png')
      div(class='collapse navbar-collapse' id='navbarSupportedContent')
        ul(class='navbar-nav')
          each page in pages
            li(class='nav-item')
              a(class='nav-link med' href=page.link) #{page.name} <hr>
      a(class='navbar-brand hidden-lg-down' href='http://www.ucsd.edu')
        img(src='/img/logo_ucsd.png')

    block main

    footer(class='unique')
      div(class='container-fluid')
        div(class='row')
          div(class='col-12 col-sm-6')
            img(class='brand-cp' src='/img/logo.png')
          div(class='col-12 col-sm-6')
            img(class='brand-ucsd' src='/img/logo_ucsd.png')

        div(class='row')
          div(class='col-12 col-md-3 col-lg-2 med')
            a(href='/') Home
              each page in pages
                a(href=page.link) #{page.name}
          div(class='col-md-9 hidden-sm-down med')
            each m in majors
              a(href='/') #{m.name}
        
