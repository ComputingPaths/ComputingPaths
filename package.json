{
  "name": "computing-paths",
  "version": "1.0.0",
  "description": "Development version of computingpaths.ucsd.edu",
  "private": true,
  "scripts": {
    "dev": "npm run clean && npm run sass && concurrently --kill-others-on-fail \"node dev.js\" \"npm run sass:watch\" \"npm run babel:watch\"",
    "sass": "node-sass --output-style compressed -o public/css styles",
    "sass:watch": "node-sass -w -r --output-style compressed -o public/css styles",
    "babel:watch": "babel scripts -w --compact=true -d public/js",
    "clean": "rimraf public/css public/js public/*.html"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "camelcase": "^4.1.0",
    "concurrently": "^3.5.0",
    "express": "^4.15.3",
    "firebase-admin": "^5.2.1",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.3",
    "rimraf": "^2.6.1"
  }
}
