    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^9.1.3",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.9.5"
