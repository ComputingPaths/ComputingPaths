{
  "name": "computing-paths",
  "version": "1.1.0",
  "description": "Development version of computingpaths.ucsd.edu",
  "private": true,
  "scripts": {
    "dev": "run-p pug:watch sass:watch babel:watch serve",
    "predev": "npm run clean",
    "build": "run-s clean pug sass babel",
    "publish": "scp -r dist/* cpaths@computingpaths.ucsd.edu:/var/www/cpaths/htdocs",
    "prepublish": "npm run build",
    "pug": "node dev.js",
    "pug:watch": "node dev.js --watch",
    "sass": "node-sass-chokidar styles --output-style compressed --output dist/css",
    "sass:watch": "node-sass-chokidar styles --watch --output dist/css",
    "babel": "babel scripts --presets=env --compact=true --no-comments --minified --out-dir dist/js",
    "babel:watch": "babel scripts --watch --out-dir dist/js",
    "clean": "rimraf dist && mkdir dist",
    "serve": "static-server --port 3000 dist"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "fs-extra": "^6.0.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "pug": "^2.0.3",
    "rimraf": "^2.6.2",
    "static-server": "^2.2.1"
  }
}
